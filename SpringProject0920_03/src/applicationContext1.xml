<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.1.xsd">

	<context:annotation-config/> <!-- 저쪽에서 @Autowired도 추가  -->
		<bean id="insaBean" class="insa.Insa"> </bean>
		<!-- <bean id="insaBean2" class="insa.Insa"> </bean> -->
		<bean id="constargBean" class="constarg.Contarg"> </bean>
		<!-- 왜  bean 태스를 기술하였지? 그것은 context:annotation-config 
		되어있는 bean의 annotation을 적용(활성화) 시키는 것이지 
		bean을 만들지는 않기 때문이다.-->
</beans>
